pipeline {
    agent any
    stages {
        stage{'Checkout'} {
            steps {
                checkout{[
                    $class: 'GITSCM',
                    branches: [[name: "*/main"]],
                    extensions: [],
                    userRemoteConfigs: [[
                        url:'https://github.co'
                    ]]
                ]}
            }
        }

        }
    }
}
